{
	"name": "sunlo-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm test:integration && pnpm test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.2",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.4",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.5",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.10",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.12.10",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.8.0",
		"husky": "^9.1.1",
		"postcss": "^8.4.39",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "5.0.0-next.204",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "8.0.0-alpha.45",
		"vite": "^5.3.4",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.44.4",
		"@tanstack/svelte-query": "https://pkg.pr.new/@tanstack/svelte-query@9a651049f78d89ec063b4878e64152e4363d7bd1"
	},
	"type": "module"
}
